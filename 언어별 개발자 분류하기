SELECT GRADE, ID, EMAIL
FROM (  SELECT ID, EMAIL,
        CASE WHEN SKILLS LIKE '%Python%' AND SKILLS LIKE '%Front End%' THEN 'A'
             WHEN SKILLS LIKE '%C#%' THEN 'B'
             WHEN SKILLS LIKE '%Front End%' THEN 'C' END AS GRADE
        FROM (  SELECT ID , EMAIL, GROUP_CONCAT(DISTINCT NAME, CATEGORY) AS SKILLS
                FROM SKILLCODES A 
                JOIN
                DEVELOPERS B ON B.SKILL_CODE & A.CODE
                GROUP BY ID, EMAIL  ) C ) D
WHERE GRADE IS NOT NULL
ORDER BY GRADE, ID
