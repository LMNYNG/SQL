SELECT ID, EMAIL, FIRST_NAME, LAST_NAME
FROM   (SELECT ID, EMAIL, FIRST_NAME, LAST_NAME, GROUP_CONCAT(DISTINCT CATEGORY) AS CATEGORY 
        FROM SKILLCODES A JOIN DEVELOPERS B ON A.CODE & B.SKILL_CODE
        GROUP BY ID, EMAIL, FIRST_NAME, LAST_NAME ) C
WHERE CATEGORY LIKE '%Front End%'
ORDER BY ID
